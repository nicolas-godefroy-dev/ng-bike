{
  "extends": "@ng-bike/ts-config-react-native",
  "include": ["."],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@screens/*": ["src/screens/*"],
      "@routes/*": ["src/routes/*"],
      "@constants/*": ["src/constants/*"],
      "@assets/*": ["src/assets/*"],
      "@hooks/*": ["src/hooks/*"],
      "@libs/*": ["src/libs/*"],
      "@services/*": ["src/services/*"]
    }
  }
}
