{
  "name": "expo-bike-sharing",
  "version": "1.0.0",
  "author": "nicolas-godefroy-dev",
  "license": "MIT",
  "private": true,
  "sideEffects": false,
  "workspaces": {
    "packages": [
      "apps/*"
    ]
  },
  "scripts": {
    "start": "npx expo start",
    "test": "yarn workspaces foreach -p run test",
    "typecheck": "yarn workspaces foreach -p run typecheck",
    "lint": "yarn workspaces foreach -p run lint",
    "lint:fix": "yarn workspaces foreach -p run lint:fix",
    "prettier:fix": "npx prettier --write .",
    "lint-staged": "lint-staged --allow-empty",
    "prepare": "husky install",
    "check-dependencies": "yarn dedupe --check",
    "start:app": "yarn workspace @expo-bike-sharing/app start"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "npx eslint --fix",
      "npx prettier --write"
    ],
    "*.{md,json}": "npx prettier --write"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.0",
    "@commitlint/config-conventional": "17.4.0",
    "@nrwl/nx-cloud": "latest",
    "@tanstack/eslint-plugin-query": "4.21.0",
    "@types/eslint": "8.4.10",
    "@types/node": "18.11.18",
    "@types/prettier": "2.7.2",
    "@typescript-eslint/eslint-plugin": "5.48.0",
    "@typescript-eslint/parser": "5.48.0",
    "@typescript-eslint/utils": "5.48.0",
    "eslint": "8.31.0",
    "eslint-config-universe": "11.1.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-expressions": "1.3.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-native": "4.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "lint-staged": "13.1.0",
    "nx": "15.4.5",
    "prettier": "2.8.2",
    "ts-node": "10.9.1",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": ">=16.15.0"
  },
  "packageManager": "yarn@3.3.1"
}
