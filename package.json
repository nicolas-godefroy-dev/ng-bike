{
  "name": "expo-bike-sharing",
  "version": "1.0.0",
  "author": "nicolas-godefroy-dev",
  "license": "MIT",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "start": "npx expo start",
    "android": "npx expo run:android",
    "ios": "npx expo run:ios",
    "eject": "npx expo eject",
    "test": "jest",
    "updateSnapshots": "jest -u --coverage=false",
    "typecheck": "tsc",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint --fix . && yar run prettier:fix",
    "prettier:fix": "npx prettier --write .",
    "lint-staged": "lint-staged --allow-empty",
    "prepare": "husky install",
    "check-dependencies": "yarn dedupe --check"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "npx eslint --fix",
      "npx prettier --write"
    ],
    "*.{md,json}": "npx prettier --write"
  },
  "dependencies": {
    "@expo-google-fonts/open-sans": "0.2.3",
